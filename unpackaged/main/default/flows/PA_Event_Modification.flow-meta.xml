    <decisions>
        <name>dec_ModificationType</name>
        <label>What are the modification?</label>
        <locationX>710</locationX>
        <locationY>134</locationY>
        <defaultConnector>
            <targetReference>ERROR_Type</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Modification</defaultConnectorLabel>
        <rules>
            <name>decout_Date_changed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>modificationType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_AP_eventModification</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_new_Appointment_Date</targetReference>
            </connector>
            <label>Date Modified</label>
        </rules>
        <rules>
            <name>decout_StaffChanged</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>modificationType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_AP_eventAssignment</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_New_Staff</targetReference>
            </connector>
            <label>Staff changed</label>
        </rules>
        <rules>
            <name>decout_Cancellation</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>modificationType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_AP_eventCancellation</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_CancellationToEvent</targetReference>
            </connector>
            <label>Cancellation</label>
        </rules>
        <rules>
            <name>decout_Show</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>modificationType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_AP_eventShow</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Complete_event</targetReference>
            </connector>
            <label>Show</label>
        </rules>
        <rules>
            <name>decout_NoShow</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>modificationType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_AP_eventNoShow</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Complete_event_when_no_show</targetReference>
            </connector>
            <label>NoShow</label>
        </rules>
    </decisions>
    <description>...PA FLOW, use to modify the of Event of the Booking</description>
    <environments>Default</environments>
    <formulas>
        <description>Transform the DateTime in Date Only</description>
        <name>ActivityDate</name>
        <dataType>Date</dataType>
        <expression>DATEVALUE({!clientBooking.StartsOn__c})</expression>
    </formulas>
    <formulas>
        <name>EndDateTime</name>
        <dataType>DateTime</dataType>
        <expression>{!clientBooking.StartsOn__c} + ({!clientBooking.Duration__c} /1440)</expression>
    </formulas>
