                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_B2C_orderConfirmationPage</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_CaptureSource</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_B2C_Newsletter</elementReference>
                </rightValue>
            </conditions>
            <conditions>
