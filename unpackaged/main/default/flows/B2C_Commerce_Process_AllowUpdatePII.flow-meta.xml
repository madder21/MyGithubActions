        </defaultConnector>
        <defaultConnectorLabel>no update</defaultConnectorLabel>
        <rules>
            <name>yes_update</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>var_CaptureSource</leftValueReference>
                <operator>EqualTo</operator>
