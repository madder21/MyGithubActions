    <decisions>
        <name>is_capture_source_exist</name>
        <label>is capture source exist?</label>
        <locationX>380</locationX>
        <locationY>134</locationY>
        <defaultConnector>
            <targetReference>ERROR</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>don&apos;t exist</defaultConnectorLabel>
        <rules>
            <name>exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_CaptureSource</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
