    <formulas>
        <name>Fx_ODGS_TaxId</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  LEN({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}) &gt; 0,
  NOT(
    REGEX({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 
          &quot;^(\\d)\\1{2}\\.\\1{3}\\.\\1{3}-\\1{2}$&quot;
    )
  ),
  AND(
    MOD(
      MOD(
        11 - MOD(
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 1, 1)) * 10 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 2, 1)) * 9 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 3, 1)) * 8 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 5, 1)) * 7 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 6, 1)) * 6 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 7, 1)) * 5 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 9, 1)) * 4 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 10, 1)) * 3 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 11, 1)) * 2,
          11
        ),
        11
      ),
      10
    ) = VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 13, 1)),
    MOD(
      MOD(
        11 - MOD(
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 1, 1)) * 11 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 2, 1)) * 10 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 3, 1)) * 9 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 5, 1)) * 8 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 6, 1)) * 7 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 7, 1)) * 6 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 9, 1)) * 5 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 10, 1)) * 4 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 11, 1)) * 3 +
          VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 13, 1)) * 2,
          11
        ),
        11
      ),
      10
    ) = VALUE(MID({!Get_Order_Delivery_Group_Summary.RI_TAX_ID__c}, 14, 1))
  )
)</expression>
    </formulas>
