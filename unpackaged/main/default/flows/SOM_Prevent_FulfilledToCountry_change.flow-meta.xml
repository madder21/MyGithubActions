<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <assignments>
        <name>Return_Back_FulfilledToCountry_To_old_value</name>
        <label>Return Back FulfilledToCountry To old value</label>
        <locationX>827</locationX>
        <locationY>548</locationY>
        <assignmentItems>
            <assignToReference>$Record.FulfilledToCountry</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record__Prior.FulfilledToCountry</elementReference>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <name>If_Country_is_not_empty</name>
        <label>If Country is not empty</label>
        <locationX>821</locationX>
        <locationY>350</locationY>
        <defaultConnector>
            <targetReference>Return_Back_FulfilledToCountry_To_old_value</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Empty</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record__Prior.FulfilledToCountry</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.FulfilledToCountry</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Empty</label>
        </rules>
    </decisions>
    <decisions>
        <name>if_only_country_changed</name>
        <label>if only country changed</label>
        <locationX>577</locationX>
        <locationY>348</locationY>
        <defaultConnectorLabel>Only Country</defaultConnectorLabel>
        <rules>
            <name>Not_only_country</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.FulfilledToCity</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.FulfilledToPostalCode</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.FulfilledToState</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.FulfilledToStreet</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>If_Country_is_not_empty</targetReference>
            </connector>
            <label>Not only country</label>
        </rules>
    </decisions>
    <description>When FilfilledToAddress changes, this flow keeps FulfilledTocountry in its old value.</description>
