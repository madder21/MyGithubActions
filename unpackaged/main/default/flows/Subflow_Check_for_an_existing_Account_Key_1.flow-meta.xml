    <description>Check for an existing Account with the same Last Name, First Name AND (Email OR Phone) - Key 1 (Subflow 1)
IF Not found
Check for an existing Account with the same Email OR Phone - Key 2 (Subflow 2)</description>
    <environments>Default</environments>
    <formulas>
        <description>Checks if an email adress is populated on the case and chooses a value</description>
        <name>formClientEmail</name>
        <dataType>String</dataType>
        <expression>IF(
         NOT(ISBLANK({!CaseId.SuppliedEmail})),
          {!CaseId.SuppliedEmail},

        IF(
               NOT(ISBLANK({!CaseId.CustomerEmail__c})), 
               {!CaseId.CustomerEmail__c},
             
               IF(
                          NOT(ISBLANK({!CaseId.SuppliedEmail__c})), 
                           {!CaseId.SuppliedEmail__c},
                      
                      IF(
                                NOT(ISBLANK({!CaseId.ContactEmail})),
                                {!CaseId.ContactEmail},
                                  &quot;&quot;
                               )
                         )
                   )
         )</expression>
    </formulas>
    <formulas>
        <description>From case LastName and FirstName</description>
        <name>formCustomerName</name>
        <dataType>String</dataType>
        <expression>IF(AND(!ISBLANK({!formLastName}), !ISBLANK({!formFirstName})),
   {!formLastName}+ &quot; &quot; +{!formFirstName},
IF(AND(!ISBLANK({!formLastName}), ISBLANK({!formFirstName})),
   {!formLastName},
IF(AND(ISBLANK({!formLastName}), !ISBLANK({!formFirstName})),
   {!formFirstName},
IF(AND(ISBLANK({!formLastName}), ISBLANK({!formFirstName})),
   {!CaseId.SuppliedName},
   &quot;No last name&quot;
))))</expression>
    </formulas>
    <formulas>
        <name>formFirstName</name>
        <dataType>String</dataType>
        <expression>IF(
       NOT(ISBLANK({!CaseId.FirstName__c})), 
      {!CaseId.FirstName__c}, 
IF(
     NOT( ISBLANK({!CaseId.SuppliedName})),
      LEFT({!CaseId.SuppliedName},Find(&quot; &quot;, {!CaseId.SuppliedName})-1),
      LEFT({!formCustomerName}, FIND(&quot; &quot;, {!formCustomerName})-1)
    )
    )</expression>
    </formulas>
    <formulas>
        <name>formLastName</name>
        <dataType>String</dataType>
        <expression>IF(
      NOT(ISBLANK({!CaseId.LastName__c})), 
      {!CaseId.LastName__c}, 
IF(
     NOT(ISBLANK({!CaseId.SuppliedName})),
     RIGHT({!CaseId.SuppliedName},LEN({!CaseId.SuppliedName})-Find(&quot; &quot;, {!CaseId.SuppliedName})),
     RIGHT({!formCustomerName}, LEN({!formCustomerName})-FIND( &quot; &quot;,{!formCustomerName}))
  )
  )</expression>
    </formulas>
    <formulas>
        <name>formPhone</name>
        <dataType>String</dataType>
        <expression>IF(
       NOT(ISBLANK({!CaseId.SuppliedPhone})),
   {!CaseId.SuppliedPhone},
     IF(
            NOT(ISBLANK({!CaseId.Phone__c})),
           {!CaseId.Phone__c},
          &quot;&quot;
                )
             )</expression>
    </formulas>
