    <decisions>
        <description>See US SOM-383 : we want to check if the quantity is sufficient in OCI compared to the current quantity ordered PLUS the quantity already allocated in existing Fulfillment Orders.</description>
        <name>Result_is_pass_or_fail</name>
        <label>Result is pass or fail</label>
        <locationX>2476</locationX>
        <locationY>2059</locationY>
        <defaultConnectorLabel>def</defaultConnectorLabel>
        <rules>
            <name>Fail_create_PE</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>L11_Full_Results.quantityRequested</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>FX_OnHand_minus_AlreadyAllocated</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_PE_for_Item</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Process_Exeption_values</targetReference>
            </connector>
            <label>❌ Fail : create PE</label>
        </rules>
        <rules>
            <name>Fail_update_PE_desc</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>L11_Full_Results.quantityRequested</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>FX_OnHand_minus_AlreadyAllocated</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_PE_for_Item</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_PE_Desc</targetReference>
            </connector>
            <label>❌ Fail : update PE desc</label>
        </rules>
        <rules>
            <name>Pass_resolve_PE</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>L11_Full_Results.quantityRequested</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>FX_OnHand_minus_AlreadyAllocated</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_PE_for_Item</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Pass</targetReference>
            </connector>
            <label>✅ Pass : resolve PE</label>
        </rules>
        <rules>
            <name>Pass</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>L11_Full_Results.quantityRequested</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>FX_OnHand_minus_AlreadyAllocated</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_PE_for_Item</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Pass_0</targetReference>
            </connector>
            <label>✅ Pass</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>FX_OnHand_minus_AlreadyAllocated</name>
        <dataType>Number</dataType>
