                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_TaskBookingAssignment</targetReference>
            </connector>
            <label>Booking to assign</label>
        </rules>
        <rules>
            <name>decout_BookingAssignmentChanged</name>
            <conditionLogic>and</conditionLogic>
