    <decisions>
        <name>dec_TaskToCreate</name>
        <label>What is the task to Create?</label>
        <locationX>842</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>No tasktype</defaultConnectorLabel>
        <rules>
            <name>decout_BookingCreation</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>taskType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.LABS_AP_Task_BookingCreated</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assign_taskBookingCreation</targetReference>
            </connector>
            <label>Booking creation</label>
        </rules>
        <rules>
            <name>decout_AssignBooking</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>taskType</leftValueReference>
                <operator>EqualTo</operator>
