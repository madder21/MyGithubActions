                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_TaskBookingRescheduled</targetReference>
            </connector>
            <label>Booking Date Modification</label>
        </rules>
        <rules>
            <name>decout_BookingReminder</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>taskType</leftValueReference>
                <operator>EqualTo</operator>
