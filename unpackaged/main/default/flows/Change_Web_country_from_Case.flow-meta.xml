            </conditions>
            <connector>
                <targetReference>Web_Country_to_EMEA</targetReference>
            </connector>
            <label>Market EMEA</label>
        </rules>
        <rules>
            <name>Market_Asia_pasific</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Market__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Asia</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Web_Country_to_Asia</targetReference>
            </connector>
            <label>Market Asia</label>
        </rules>
