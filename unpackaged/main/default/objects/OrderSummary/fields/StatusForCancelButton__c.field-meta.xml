<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StatusForCancelButton__c</fullName>
    <externalId>false</externalId>
    <formula>IF(NOT(
ISPICKVAL(Status, &quot;Partially Fulfilled&quot;) ||
ISPICKVAL(Status, &quot;Fulfilled&quot;) ||
ISPICKVAL(Status, &quot;Canceled&quot;)
),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>StatusForCancelButton</label>
    <type>Checkbox</type>
</CustomField>
