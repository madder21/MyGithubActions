<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LocalOrTourist__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(TEXT(Client__r.Country__pc)),  
				&quot;Unlocalized&quot;, 
			 IF(TEXT(Client__r.Country__pc) = TEXT(Store__r.Country__c),
							&quot;Local&quot;, 
							&quot;Tourist&quot;
		) 	
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Local or Tourist ?</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
