<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fulfilled_To_Country_ISO_2</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    OR(
        ISBLANK(FulfilledToCountry),
        NOT(REGEX(FulfilledToCountry, &quot;^[A-Z]{2}$&quot;))
    ),
    $Profile.Name = &quot;System Administrator&quot;
)</errorConditionFormula>
    <errorMessage>The value should be in the format ISO 2 and should not be empty.</errorMessage>
</ValidationRule>
