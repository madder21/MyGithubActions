<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GrossUnitPriceFormula__c</fullName>
    <externalId>false</externalId>
    <formula>IF(CONTAINS(TEXT(GrossUnitPrice), &quot;.&quot;),SUBSTITUTE(TEXT(GrossUnitPrice) , &quot;.&quot;, &quot;,&quot;), TEXT(GrossUnitPrice)+&quot;,00&quot;) +&quot; &quot; + TEXT( CurrencyIsoCode )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Gross Unit Price Formula</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
