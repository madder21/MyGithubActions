<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CheckSKU</fullName>
    <active>true</active>
    <errorConditionFormula>NOT (  REGEX( SKU__c , &quot;[0-9]+&quot;) )</errorConditionFormula>
    <errorDisplayField>SKU__c</errorDisplayField>
    <errorMessage>SKU must contain only numbers.</errorMessage>
</ValidationRule>
